template< class Key,
          class Value,
          class KeyOfValue,
          class HashFunct,
          class LessKey >
inline
DashTable< Key, Value, KeyOfValue, HashFunct, LessKey >::DashTable(
  ChunkMgr &		chunkMgr,
  const char *		indexFileName,
  ios::open_mode	mode,
  bool			create,
  unsigned short	permMask
  )
  : DashTableBase( chunkMgr, indexFileName, mode, permMask, create )
{
}

template< class Key,
          class Value,
          class KeyOfValue,
          class HashFunct,
          class LessKey >
inline
HashTableBase::size_type
DashTable< Key, Value, KeyOfValue, HashFunct, LessKey >::getNodeSize( void )
{
  return( sizeof( DashNode ) );
}


